{
  "extends": "airbnb",
  "ecmaFeatures": {
    "classes": true,
    "jsx": true
  },
  "parser": "babel-eslint",
  "rules": {

    // Remove trailing comma error.
    "comma-dangle": 0,

    // Remove .jsx filename for JSX restriction.
    "react/jsx-filename-extension": ["error", { "extensions": [".js", ".jsx"] }]
  },
  "env": {
    "jest": true
  }
}
